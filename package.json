{
  "name": "pose-est-front",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "react-router build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "wrangler dev",
    "deploy": "npm run build && wrangler deploy",
    "typecheck": "tsc --noEmit",
    "typegen": "react-router typegen",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\" --ignore-path .gitignore",
    "clean": "rm -rf node_modules dist .react-router build",
    "reinstall": "npm run clean && npm install",
    "test:all": "npm run typegen && npm run typecheck && npm run lint && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.3",
    "axios": "^1.7.9",
    "isbot": "^5.1.32",
    "lucide-react": "^0.469.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-player": "^2.16.0",
    "react-router": "^7.12.0",
    "react-router-dom": "^7.12.0",
    "winston": "^3.14.2",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.17.1",
    "@cloudflare/workers-types": "^4.20260103.0",
    "@react-router/dev": "^7.12.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.24",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16",
    "wrangler": "^4.59.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
